---
name: <%= @name %>
package_key: <%= options[:package_key] %>
repo_key: <%= options[:repo_key] %>
srcdest: sources
logdest: logs
pkgbuild: PKGBUILDs
builds:
<%- @archs.each do |arch| -%>
  <%= arch %>:
    makepkg: etc/makepkg.<%= arch %>.conf
    pacman: etc/pacman.<%= arch %>.conf
<%- end -%>
s3:
<%- if options[:s3] -%>
  access_key_id:
  secret_access_key:
  bucket:
  region:
  write_options:
    # :acl: :public_read
    # :reduced_redundancy: false
    # :server_side_encryption: :aes256
<%- end -%>
