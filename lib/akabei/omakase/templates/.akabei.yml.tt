---
name: <%= @name %>
package_key: <%= options[:package_key] %>
repo_key: <%= options[:repo_key] %>
srcdest: sources
logdest: logs
pkgbuild: PKGBUILDs
builds:
<%- @archs.each do |arch| -%>
  <%= arch %>:
    makepkg: etc/makepkg.<%= arch %>.conf
    pacman: etc/pacman.<%= arch %>.conf
<%- end -%>
s3:
<%- if options[:s3] -%>
  access_key_id:
  secret_access_key:
  bucket:
  region:
<%- end -%>
